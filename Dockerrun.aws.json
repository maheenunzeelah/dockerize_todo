{
    "AWSEBDockerrunVersion":"1",
    "containerDefinitions":[
        {
            "name":"client",
            "image": "maheen299/multi-client",
            "hostname": "client",
            "memory": 100,
            "essential": false
        },
        {
            "name":"server",
            "image": "maheen299/multi-server",
            "hostname": "api",
            "memory": 100,
            "essential":false
        },
        {
			"essential": true,
			"image": "redis:latest",
			"memory": 100,
			"name": "redis",
			"portMappings": [{
				"containerPort": 6379,
				"hostPort": 6379
			}]
		},
        {
            "name":"nginx",
            "image": "maheen299/multi-nginx",
            "hostname":"nginx",
            "essential":true,
            "memory": 100,
            "portMappings":[
                {
                    "hostPort":80,
                    "containerPort":80
                }
            ],
            "links":["client","server"]
        }    
    ]
}