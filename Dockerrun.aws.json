{
    "AWSEBDockerrunVersion":"1",
    "containerDefinitions":[
        {
            "name":"client",
            "image": "maheen299/multi-client",
            "hostname": "client",
            "memory": 100,
            "essential": false,
            "Ports": [
                {
                    "HostPort": 3000,
                    "ContainerPort": 3000
                }
            ]
        },
        {
            "name":"server",
            "image": "maheen299/multi-server",
            "hostname": "api",
            "memory": 100,
            "essential":false,
            "Ports": [
                {
                    "HostPort": 5000,
                    "ContainerPort": 5000
                }
            ]
        },
        {
			"essential": true,
			"image": "redis:latest",
			"memory": 100,
			"name": "redis",
			"Ports": [{
				"ContainerPort": 6379,
				"HostPort": 6379
			}]
		},
        {
            "name":"nginx",
            "image": "maheen299/multi-nginx",
            "hostname":"nginx",
            "essential":true,
            "memory": 100,
            "Ports":[
                {
                    "HostPort":80,
                    "ContainerPort":80
                }
            ],
            "links":["client","server"]
        }    
    ]
}